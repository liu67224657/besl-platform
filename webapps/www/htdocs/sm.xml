<?xml version="1.0" encoding="UTF-8"?>
<urlset>
    <!--表示www.domain.com 下的一组pattern 或url对应关系-->
    <!-- news-->
    <url>
        <loc>http://www.joyme.com/</loc>
        <!-- 表示pc站点的网址首页 -->
        <data>
            <display>
                <pc_url_pattern>http://www.joyme.com/news/asiangames/(\d+)/(\d+).html</pc_url_pattern>
                <!-- 表示该PC页的url pattern-->
                <xhtml_url_pattern>http://m.joyme.com/news/asiangames/${1}/${2}.html</xhtml_url_pattern>
                <!-- 一组PC pattern 可能含有多个版式的手机页pattern，视自身情况提交 -->
            </display>
        </data>
    </url>
